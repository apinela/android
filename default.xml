<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Include AOSP -->
  <include name="android-6.0.1_r22.xml" />

  <remote  name="github"
           fetch="https://github.com/" />

  <!-- Replace build project to build kernel from the source -->
  <remove-project name="platform/build" />
  <project path="build" name="grouper-aosp/android_build" remote="github" revision="aosp-6.0" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Grouper device and kernel -->
  <project path="device/asus/grouper" name="apinela/android_device_asus_grouper" remote="github" revision="aosp-6.0" groups="device,grouper" />
  <project path="device/asus/tilapia" name="apinela/android_device_asus_tilapia" remote="github" revision="aosp-6.0" groups="device,grouper" />
  <project path="kernel/asus/grouper" name="apinela/android_kernel_asus_grouper" remote="github" revision="grouper/3.4/dev" groups="device,grouper" />

  <!-- Grouper vendor repositories -->
  <project path="vendor/asus" name="grouper-aosp/proprietary_vendor_asus" remote="github" revision="aosp-6.0" />
  <project path="vendor/broadcom" name="grouper-aosp/proprietary_vendor_broadcom" remote="github" revision="aosp-6.0" />
  <project path="vendor/invensense" name="grouper-aosp/proprietary_vendor_invensense" remote="github" revision="aosp-6.0" />
  <project path="vendor/nvidia" name="grouper-aosp/proprietary_vendor_nvidia" remote="github" revision="aosp-6.0" />
  <project path="vendor/nxp" name="grouper-aosp/proprietary_vendor_nxp" remote="github" revision="aosp-6.0" />
  <project path="vendor/widevine" name="grouper-aosp/proprietary_vendor_widevine" remote="github" revision="aosp-6.0" />

</manifest>
