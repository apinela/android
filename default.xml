<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="Grouper-AOSP"
           fetch="https://github.com/Grouper-AOSP/" />

  <!-- Include AOSP -->
  <include name="android-6.0.1_r22.xml" />

  <!-- Replace build project to build kernel from the source -->
  <remove-project name="platform/build" />
  <project path="build" name="android_build" remote="Grouper-AOSP" revision="aosp-6.0" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Grouper device and kernel -->
  <project path="device/asus/grouper" name="android_device_asus_grouper" remote="Grouper-AOSP" revision="aosp-6.0" groups="device,grouper" />
  <project path="device/asus/tilapia" name="android_device_asus_tilapia" remote="Grouper-AOSP" revision="aosp-6.0" groups="device,grouper" />
  <project path="kernel/asus/grouper" name="android_kernel_asus_grouper" remote="Grouper-AOSP" revision="aosp-6.0" groups="device,grouper" />

  <!-- Grouper vendor repositories -->
  <project path="vendor/asus" name="proprietary_vendor_asus" remote="Grouper-AOSP" revision="aosp-6.0" />
  <project path="vendor/broadcom" name="proprietary_vendor_broadcom" remote="Grouper-AOSP" revision="aosp-6.0" />
  <project path="vendor/invensense" name="proprietary_vendor_invensense" remote="Grouper-AOSP" revision="aosp-6.0" />
  <project path="vendor/nvidia" name="proprietary_vendor_nvidia" remote="Grouper-AOSP" revision="aosp-6.0" />
  <project path="vendor/nxp" name="proprietary_vendor_nxp" remote="Grouper-AOSP" revision="aosp-6.0" />
  <project path="vendor/widevine" name="proprietary_vendor_widevine" remote="Grouper-AOSP" revision="aosp-6.0" />

</manifest>
